{
    "env": {
        "browser": true,
        "es6": true,
        "commonjs": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "emcaVersion": 2018,
        "sourceType": "module"
    },
    "rules": { // 0 = off, 1 = warn, 2 = error
        "@typescript-eslint/no-empty-function": 1
    }
}